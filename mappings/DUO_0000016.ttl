PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX odrl: <http://www.w3.org/ns/odrl/2/>
PREFIX : <http://example.com/>

:DUO_0000016 a odrl:Set ;
    rdfs:label "DUO_0000016: This data use modifier indicates that use is limited to genetic studies only (GSO - genetic studies only)" ;
    odrl:permission [
        odrl:target :TemplateDataset ;
        odrl:constraint [
            odrl:leftOperand odrl:purpose ;
            odrl:operator odrl:isA ;
            odrl:rightOperand :GeneticStudies
        ]

        # See comments about interpreting "limited to"
    ] .
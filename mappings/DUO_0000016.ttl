PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX odrl: <http://www.w3.org/ns/odrl/2/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX obo: <http://purl.obolibrary.org/obo/>
PREFIX : <https://w3id.org/duodrl#>

:DUO_0000016 a odrl:Set ;
    rdfs:label "DUO_0000016: This data use modifier indicates that use is limited to genetic studies only (GSO - genetic studies only)" ;
    skos:exactMatch obo:DUO_0000016 ;
    odrl:permission [
        odrl:target :TemplateDataset ;
        odrl:constraint [
            odrl:leftOperand odrl:purpose ;
            # Question: how to determine conflict between permissions that have the only operator and other permissions?
            odrl:operator :only ;
            odrl:rightOperand :GS
        ]
    ] .
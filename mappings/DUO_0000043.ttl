PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX odrl: <http://www.w3.org/ns/odrl/2/>
PREFIX : <http://example.com/>

:DUO_0000043 a odrl:Set ;
    rdfs:label "DUO_0000043: This data use modifier indicates that use is allowed for clinical use and care (CC - clinical care use)" ;
    odrl:permission [
        odrl:target :TemplateDataset ;
        odrl:constraint [
            odrl:leftOperand odrl:purpose ;
            odrl:operator odrl:isA ;
            odrl:rightOperand :ClinicalCareUse
        ]

        # Same comment as general research constraint
    ] .
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX odrl: <http://www.w3.org/ns/odrl/2/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX : <http://example.com>

:DUO_0000025 a odrl:Set ;
    rdfs:label "DUO_0000025: This data use modifier indicates that use is approved for a specific number of months. (TS - time limit on use)" ;
    odrl:permission [
        odrl:target :TemplateDataset ;
        odrl:constraint [
            odrl:leftOperand odrl:elapsedTime ;
            odrl:operator odrl:lteq ;
            odrl:rightOperand "PmM"^^xsd:duration
        ]

        # See constraints where templates are used, similar to publication moratorium with a specific date
    ] .
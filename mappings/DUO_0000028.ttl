PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX odrl: <http://www.w3.org/ns/odrl/2/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX obo: <http://purl.obolibrary.org/obo/>
PREFIX : <https://w3id.org/duodrl#>

:DUO_0000028 a odrl:Set ;
    rdfs:label "DUO_0000028: This data use modifier indicates that use is limited to use within an approved institution. (IS - institution specific restriction)" ;
    skos:exactMatch obo:DUO_0000028 ;
    odrl:permission [
        odrl:assignee [ 
            a odrl:Party ;
            obo:DUO_0000010 :TemplateInstitution ;
        ] ;
        odrl:target :TemplateDataset
    ] .
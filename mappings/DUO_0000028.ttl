PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX odrl: <http://www.w3.org/ns/odrl/2/>
PREFIX : <http://example.com>

:DUO_0000028 a odrl:Set ;
    rdfs:label "DUO_0000028: This data use modifier indicates that use is limited to use within an approved institution. (IS - institution specific restriction)" ;
    odrl:permission [
        odrl:assignee [ 
            a odrl:PartyCollection ;
            odrl:source :ApprovedInstitution ;
        ] ;
        odrl:target </jane-doe/data/>
    ] .
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX odrl: <http://www.w3.org/ns/odrl/2/>
PREFIX : <http://example.com/>

:DUO_0000042 a odrl:Set ;
    rdfs:label "DUO_0000042: This data use permission indicates that use is allowed for general research use for any research purpose (GRU - general research use)" ;
    odrl:permission [
        odrl:target :TemplateDataset ;
        odrl:constraint [
            odrl:leftOperand odrl:purpose ;
            odrl:operator odrl:isA ;
            odrl:rightOperand :GeneralResearch
        ]

        # Do we need to constrat the language used here, i.e. "is allowed for" vs the more restrictive language, i.e. "is limited to" ?
        # Is the interpretation here that allowed would result in a set of rulse with condition OR, whereas limited would prohibit any other rule to be present (or would it be AND?)
    ] .
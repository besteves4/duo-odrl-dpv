PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX odrl: <http://www.w3.org/ns/odrl/2/>
PREFIX dpv: <http://www.w3.org/ns/dpv#>
PREFIX oac: <https://w3id.org/oac/>
PREFIX : <http://example.com>

:DUO_0000022 a odrl:Policy ;
    odrl:profile oac: ;
    rdfs:label "DUO_0000022: This data use modifier indicates that use is limited to within a specific geographic region. (GS - geographical restriction)" ;
    rdfs:comment "An IRI MUST be used to represent the geospatial area, e.g., ISO 3166 Country Codes - <https://www.iso.org/obp/ui/#iso:code:3166:BE> for Belgium";
    odrl:permission [
        a odrl:Permission ;
        odrl:assigner [ a oac:DataSubject ; dpv:hasName "Jane Doe" ] ;
        odrl:action oac:Use ;
        odrl:target </jane-doe/data/> ;
        odrl:constraint [
            odrl:leftOperand odrl:spatial ;
            odrl:operator odrl:eq ;
            odrl:rightOperand <https://www.iso.org/obp/ui/#iso:code:3166:BE>
        ]
    ] .
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX odrl: <http://www.w3.org/ns/odrl/2/>
PREFIX : <http://example.com/>

:DUO_0000027 a odrl:Set ;
    rdfs:label "DUO_0000027: This data use modifier indicates that use is limited to use within an approved project (PS - project specific restriction)" ;
    odrl:permission [
        odrl:target :TemplateDataset ;
        odrl:constraint [
            odrl:leftOperand :Project ;
            odrl:operator odrl:isA ;
            odrl:rightOperand :ApprovedProject
        ]

        # Same comments as the constraint regarding approved users
    ] .